    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@qdrant/js-client-rest": "^1.16.2",
    "@types/compression": "^1.8.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.15.6",
    "@willsoto/nestjs-prometheus": "^6.0.2",
